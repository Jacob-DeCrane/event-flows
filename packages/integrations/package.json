{
  "name": "@eventflows/integrations",
  "version": "0.1.0",
  "description": "Production-ready integrations for EventFlows (AWS, PostgreSQL, and more)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "event-sourcing",
    "cqrs",
    "ddd",
    "domain-driven-design",
    "integrations",
    "aws",
    "postgresql",
    "eventbridge",
    "dynamodb",
    "typescript",
    "framework-agnostic"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jacob-DeCrane/event-flows.git",
    "directory": "packages/integrations"
  },
  "homepage": "https://jacob-decrane.github.io/event-flows/",
  "bugs": {
    "url": "https://github.com/Jacob-DeCrane/event-flows/issues"
  },
  "peerDependencies": {
    "@eventflows/core": "^0.1.0"
  },
  "devDependencies": {
    "@eventflows/core": "^0.1.0",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
